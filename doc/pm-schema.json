{
  "@context": "http://qiicr.org/dcmqi/contexts/dcmqi.jsonld",
  "id": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/pm-schema.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "required": [
    "DerivedPixelContrast",
    "AnatomicRegionCode",
    "FrameLaterality"
  ],
  "additionalProperties": false,
  "properties": {
    "@schema": {"type" : "string"},
    "SeriesDescription": {
      "allOf": [
        {
          "$ref": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/common-schema.json#/definitions/LO"
        }
      ],
      "default": "Segmentation"
    },
    "SeriesNumber": {
      "allOf": [
        {
          "$ref": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/common-schema.json#/definitions/IS"
        }
      ],
      "default": "300"
    },
    "InstanceNumber": {
      "allOf": [
        {
          "$ref": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/common-schema.json#/definitions/IS"
        }
      ],
      "default": "1"
    },
    "BodyPartExamined": {
      "allOf": [
        {
          "$ref": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/common-schema.json#/definitions/CS"
        }
      ],
      "default": ""
    },
    "QuantityValueCode": {
      "$ref": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/common-schema.json#/definitions/codeSequence"
    },
    "MeasurementUnitsCode": {
      "$ref": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/common-schema.json#/definitions/codeSequence"
    },
    "MeasurementMethodCode": {
      "$ref": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/common-schema.json#/definitions/codeSequence"
    },
    "ModelFittingMethodCode": {
      "$ref": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/common-schema.json#/definitions/codeSequence"
    },
    "SourceImageDiffusionBValues": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "RealWorldValueSlope": {
      "$ref": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/common-schema.json#/definitions/FD",
      "default": "1.0"
    },
    "DerivedPixelContrast": {
      "enum": [
        "ADDITION",
        "DIVISION",
        "MASKED",
        "MAXIMUM",
        "MEAN",
        "MINIMUM",
        "MTT",
        "MULTIPLICATION",
        "RCBF",
        "RCBV",
        "RESAMPLED",
        "STD_DEVIATION",
        "SUBTRACTION",
        "T_TEST",
        "TTP",
        "Z_SCORE",
        "MIXED",
        "ADC",
        "DIFFUSION",
        "DIFFUSION_ANISO",
        "DIFFUSION_ATTND",
        "DIFFUSION_ISO",
        "FAT",
        "FAT_FRACTION",
        "FIELD_MAP",
        "IN_PHASE",
        "METABOLITE_MAP",
        "NEI",
        "OUT_OF_PHASE",
        "PERFUSION_ASL",
        "R_COEFFICIENT",
        "R2_MAP",
        "R2_STAR_MAP",
        "RHO",
        "SCM",
        "SNR_MAP",
        "T1_MAP",
        "T2_STAR_MAP",
        "T2_MAP",
        "TCS",
        "TEMPERATURE",
        "VELOCITY",
        "WATER",
        "WATER_FRACTION"
      ]
    },
    "AnatomicRegionCode": {
      "$ref": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/common-schema.json#/definitions/codeSequence"
    },
    "FrameLaterality": {
      "$ref": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/common-schema.json#/definitions/CS",
      "default": "U"
    },
    "CodingSchemeIdentificationSequence": {
      "type": "array",
      "items": {
        "$ref": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/common-schema.json#/definitions/codingSchemeIdentificationSequenceItem"
      }
    }
  }
}
